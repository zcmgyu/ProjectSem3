@using Model.ViewModel;
@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var collection = ViewBag.NewCollection as List<ProductDetailGeneral>;
    var featured = ViewBag.NewFeatured as List<ProductDetailGeneral>;
    string[] classType = { "primary-photo", "secondary-photo" };
}


<!-- SLIDER-AREA START -->
@{ Html.RenderAction("TopSlide");}
<!-- SLIDER-AREA END -->
<!-- BANNER-STYLE-2-AREA START -->
<div class="banner-style-2-area fix">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-12 col-lg-offset-3 col-md-offset-3 padding-0">
                <div class="banner-style-2">
                    <div class="single-banner-style-2 banner-brief-1">
                        <div class="left-banner-img">
                            <img class="" src="~/Content/Client/img/banner/3.png" alt="" />
                        </div>
                        <div class="left-banner-brief">
                            <h3>new fashion collection</h3>
                            <h4>passages dress</h4>
                            <h4>$50.00</h4>
                            <p>Contrary to popular belief, Lorem Ipsum is not simply random text. It has roots in a piece of classical.</p>
                            <a href="#" class="shop-now">Shop Now</a>
                        </div>
                    </div>
                    <div class="single-banner-style-2 banner-brief-2">
                        <div class="right-banner-brief">
                            <h3>exclusive for men</h3>
                            <h4>passages dress</h4>
                            <h4>$50.00</h4>
                            <p>Contrary to popular belief, Lorem Ipsum is not simply random text. It has roots in a piece of classical.</p>
                            <a href="#" class="shop-now">Shop Now</a>
                        </div>
                        <div class="right-banner-img">
                            <img class="" src="~/Content/Client/img/banner/4.png" alt="" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- BANNER-STYLE-2-AREA END -->
<!-- NEW-COLLECTION START -->
<div class="new-collection-area fix style-2 margin-bottom-80">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="section-title-2 bordr-rights">
                    <h2 class="border-left-rights">New Collection</h2>
                </div>
            </div>
        </div>
    </div>
    <div class="new-collection active-new-collection-2">

        <!-- Single-product start -->
        @foreach (var item in collection)
            {
            <div class="single-product">
                <div class="product-photo">
                    <a href="/Product/Detail/@item.ID">
                        @for (var i = 0; i < 2; i++)
                        {
                            if ((item.ListImage.Count - 1) < i)
                            {
                                break;
                            }
                            <img class="@classType[i]" src="@Url.Content(item.ListImage[i].Url)" alt="" />
                        }

                        @*<img class="secondary-photo" src="~/Content/Client/img/new-collection/new-2/5.jpg" alt="" />*@
                    </a>
                    <div class="pro-action">
                        <a href="#" class="action-btn"><i class="sp-heart"></i><span>Wishlist</span></a>
                        <a href="#" class="action-btn"><i class="sp-shopping-cart"></i><span>Add to cart</span></a>
                        <a href="#" class="action-btn"><i class="sp-compare-alt"></i><span>Compare</span></a>
                    </div>
                </div>
                <div class="product-brief">
                    <div class="pro-rating">
                        <a href="#"><i class="sp-star rating-1"></i></a>
                        <a href="#"><i class="sp-star rating-1"></i></a>
                        <a href="#"><i class="sp-star rating-1"></i></a>
                        <a href="#"><i class="sp-star rating-1"></i></a>
                        <a href="#"><i class="sp-star rating-2"></i></a>
                    </div>
                    <h2><a href="/Product/Detail/@item.ID">@item.Name</a></h2>
                    <h3 class="s-price-box">
                        @if (item.PromotionPrice == 0 || item.PromotionPrice == null)
                        {
                            <span>$@item.Price</span>
                        }
                        else
                        {
                            <span class="old-price">$@item.Price</span>
                            <span class="new-price">$@item.PromotionPrice</span>
                        }
                    </h3>
                </div>
            </div>
        }
        <!-- Single-product end -->

    </div>
</div>
<!-- NEW-COLLECTION END -->
